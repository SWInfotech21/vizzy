                                        <input type="radio" class="btn-check" name="btnradio" id="btnradio2" />
                                        <label class="btn btn-outline-primary" for="btnradio2" dmx-on:click="varSubMenu.setValue('Plan')">Plan</label>

                                        <input type="radio" class="btn-check" name="btnradio" id="btnradio3" />
                                        <label class="btn btn-outline-primary" for="btnradio3" dmx-on:click="varSubMenu.setValue('Notifications')">Notifications</label>

                                        <input type="radio" class="btn-check" name="btnradio" id="btnradio4" />
                                        <label class="btn btn-outline-primary" for="btnradio4" dmx-on:click="varSubMenu.setValue('Security')">Security</label>

                                        <input type="radio" class="btn-check" name="btnradio" id="btnradio5" />
                                        <label class="btn btn-outline-primary" for="btnradio5" dmx-on:click="varSubMenu.setValue('Billing')">Billing</label>
                                    </div>
                                </div>



                                <!-- <ul class="nav nav-pills flex-column flex-md-row mb-4">
                                    <li class="nav-item" dmx-class:bg-menu-theme="(varSubMenu.value!='Profile')" dmx-on:click="varSubMenu.setValue('Profile')">
