{
  "routes": [
    {
      "path": "/",
      "page": "index",
      "routeType": "page",
      "layout": "main"
    },
    {
      "path": "/pricing",
      "page": "pricing",
      "layout": "main"
    },
    {
      "path": "/contact-us",
      "page": "contact-us",
      "layout": "main"
    },
    {
      "path": "/reset-password/:token",
      "page": "portal/register",
      "layout": "outer-main"
    },
    {
      "path": "/portal/login",
      "page": "portal/login",
      "layout": "outer-main"
    },
    {
      "path": "/portal/forgot-password",
      "page": "portal/forgot-password",
      "layout": "outer-main"
    },
    {
      "path": "/portal/dashboard",
      "page": "portal/dashboard",
      "layout": "dashboard"
    },
    {
      "path": "/portal/my-profile",
      "page": "portal/my-profile",
      "layout": "dashboard"
    },
    {
      "path": "/portal/tasks",
      "page": "portal/tasks",
      "layout": "dashboard"
    },
    {
      "path": "/reset-pass/:token",
      "page": "reset-pass",
      "layout": "reset-password",
      "exec": "/api/User/CheckResetToken"
    },
    {
      "path": "/*",
      "page": "pages-error",
      "layout": "main"
    }
  ],
  "layouts": {
    "dashboard": {
      "exec": "/api/PageAccess/UserAccess"
    },
    "reset-password": {
      "exec": ""
    }
  }
}